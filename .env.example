# Halapi API Configuration
# Copy this file to .env and fill in your values

# Proxy target for development (Vite will proxy /api/halap/* to this URL)
VITE_HALAPI_PROXY_TARGET=https://haldev.cybermeet.fr

# Your Halapi API token (starts with hap_sk_live_)
VITE_HALAPI_TOKEN=hap_sk_live_YOUR_TOKEN_HERE
